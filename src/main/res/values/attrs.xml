<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MyTextView">
        <attr name="fontName" format="string" />
    </declare-styleable>

    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference" />
        <attr name="metaButtonBarButtonStyle" format="reference" />
    </declare-styleable>

    <!-- Custom Attributes for Progress Wheel -->
    <declare-styleable name="ProgressWheel">
        <attr name="text" format="string" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="barColor" format="color" />
        <attr name="rimColor" format="color" />
        <attr name="rimWidth" format="dimension" />
        <attr name="spinSpeed" format="integer" />
        <attr name="circleColor" format="color" />
        <attr name="barWidth" format="dimension" />
        <attr name="barLength" format="dimension" />
        <attr name="delayMillis" format="dimension" />
        <attr name="contourColor" format="color" />
        <attr name="contourSize" format="float" />
    </declare-styleable>

    <!-- Custom Attributes for VizoRoundedLayout -->
    <declare-styleable name="VizoRoundedLayout">
        <attr name="corner_radius" format="dimension" />
    </declare-styleable>

</resources>